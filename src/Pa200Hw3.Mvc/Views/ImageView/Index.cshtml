@model ImageViewViewModel

@{
    ViewData["Title"] = "Processed Images";
}

<h1>Processed Images</h1>

<div class="row">
    @for (int i = 0; i < Model.ImageUrls.Count; i++)
    {
        <div class="col-md-4 mb-3">
            <img src="@Model.ImageUrls[i]" class="img-fluid" style="max-width: 300px; height: auto;" alt="Processed Image @(i + 1)" />
        </div>
        @if ((i + 1) % 3 == 0)
        {
            @Html.Raw("</div><div class=\"row\">")
        }
    }
</div>